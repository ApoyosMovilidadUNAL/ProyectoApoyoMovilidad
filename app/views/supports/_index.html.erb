<p id="notice"><%= notice %></p>

<h1>Supports</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Route</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @request.support.each do |support| %>
      <tr>
        <td><%= support.name %></td>
        <td><%= support.route %></td>
        <td><%= link_to 'Show', support %></td>
        <td><%= link_to 'Edit', edit_support_path(support) %></td>
        <td><%= link_to 'Destroy', support, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Support', new_student_support_path %>

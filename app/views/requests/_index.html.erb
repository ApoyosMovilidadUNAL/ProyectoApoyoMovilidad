<p id="notice"><%= notice %></p>

<h1>Requests</h1>

<table>
  <thead>
    <tr>
      <th>Req estado</th>
      <th>Req descripcion</th>
      <th>Student</th>
      <th>Professor</th>
      <th>Document</th>
      <th>Support</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.req_estado %></td>
        <td><%= request.req_descripcion %></td>
        <td><%= request.student_id %></td>
        <td><%= request.professor_id %></td>
        <td><%= request.document_id %></td>
        <td><%= request.support_id %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request', new_request_path %>

<%= will_paginate @Request %>

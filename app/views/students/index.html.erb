<p id="notice"><%= notice %></p>

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>Stu name</th>
      <th>Stu pass</th>
      <th>Stu email</th>
      <th>Stu rol</th>
      <th>Stu identification</th>
      <th>Stu facultad</th>
      <th>Stu carrera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.stu_name %></td>
        <td><%= student.stu_email %></td>
        <td><%= student.stu_rol %></td>
        <td><%= student.stu_identification %></td>
        <td><%= student.stu_facultad %></td>
        <td><%= student.stu_carrera %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_student_path %>

<%= will_paginate @Student %>
